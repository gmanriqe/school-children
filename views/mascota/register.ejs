<div class="container">
    <div class="row">
        <div class="panel-default">
            <div class="panel-heading">
                <a href="/formtipomascota" class="btn btn-primary">+ Tipo Mascota</a>
                <a href="/formpropietarios" class="btn btn-primary">+ Propietario</a>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3>Registrar Mascota</h3>
            </div>
            <div class="panel-body">
                <form action="/formregisterpet" method="POST" enctype="multipart/form-data">
                    <div class="form-group">
                        <div class="row">
                            <div class="col-xs-4 col-md-2">
                                <label for="propietario">Propietario</label>
                            </div>
                            <div class="col-xd-8 col-md-10">
                                <select name="propietario" id="propietario" class="form-control">
                                    <% props.forEach(function(prop){%>
                                    <option value="<%= prop.id %>"><%= prop.nombre %></option>
                                    <% }) %>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-xs-4 col-md-2">
                                <label for="tipomascota">Tipo mascota</label>
                            </div>
                            <div class="col-xd-8 col-md-10">
                                <select name="tipomascota" id="tipomascota" class="form-control">
                                    <% regs.forEach(function(reg){%>
                                    <option value="<%= reg.id %>"><%= reg.nombre %></option>
                                    <% }) %>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-xs-4 col-md-2">
                                <label for="nombre">Nombre</label>
                            </div>
                            <div class="col-xd-8 col-md-10">
                                <input type="text" name="nombre" id="nombre" class="form-control" required>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-xs-4 col-md-2">
                                <label for="raza">Raza</label>
                            </div>
                            <div class="col-xd-8 col-md-10">
                                <input type="text" name="raza" id="raza" class="form-control" required>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-xs-4 col-md-2">
                                <label for="sexo">Sexo</label>
                            </div>
                            <div class="col-xd-8 col-md-10">
                                <input type="text" name="sexo" id="sexo" class="form-control" required>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col-xs-4 col-md-2">
                                <label for="urlfoto">Foto Mascota</label>
                            </div>
                            <div class="col-xd-8 col-md-10">
                                <input type="file" name="urlfoto" id="urlfoto" class="form-control-file" required>
                            </div>
                        </div>
                    </div>
                    <div class="form-group text-right">
                        <input type="submit" class="btn btn-success" value="Guardar">
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>