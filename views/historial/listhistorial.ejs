<div class="container">
    <div class="row">
        <div class="panel-default">
            <div class="panel-heading">
                <a href="/addHistorialClinico" class="btn btn-primary pull-right">+Nuevo</a>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>
        
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3>Historial Clinico</h3>
            </div>
            <div class="panel-body">
                <table class="table table-striped">
                    <thead>
                        <tr>
                            <th>Mascota</th>
                            <th>Motivo Consulta</th>
                            <th>Fecha atencion</th>
                            <th>Consulta (S/.)</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% regs.forEach(function(reg){ %>
                            <tr>
                                <td><%= reg.pet.nombre%></td>
                                <td><%= reg.motivoconsulta%></td>
                                <td><%= reg.fecatencion%></td>
                                <td><%= reg.precioconsulta%></td>
                            </tr>
                        <% }) %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>